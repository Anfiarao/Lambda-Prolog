%% calc_Eq.elpi


isType (propEq A AA1 AA2) propS ext :-
    isa AA1 A ext,
    isa AA2 A ext,
    isType A set ext.

isType (propEq A AA1 AA2) prop ext :-
    isa AA1 A ext,
    isa AA2 A ext,
    isType A col ext.


of (eqTrue C Cc) (propEq C Cc Cc) ext
    :-  of Cc C ext
    .


conv A B :- of _ (propEq _ A B).

conv A B :- locDef _ (propEq _ A B) _ .
