%% calc_Eq.elpi

isType (propEq A AA1 AA2) propS ext :-
    isa AA1 A ext,
    isa AA2 A ext,
    isType A set ext.

isType (propEq A AA1 AA2) prop ext :-
    isa AA1 A ext,
    isa AA2 A ext,
    isType A col ext.


of (eqTrue C Cc) (propEq C Cc Cc) ext
    :-  of Cc C ext
    .

%dstep Cc1 Cc2 :- equJ Cc1 Cc2.


equJ Cc1 Cc2
    :-  locDef H (propEq C Cc1 Cc2) M
    ,   of H (propEq C Cc1 Cc2) ext
    .

