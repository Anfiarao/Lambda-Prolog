            )
            )
        )
        ) col ext).


testE
 :- spy( 
        isaT (
        forall singleton x\
        forall (fun (propEq singleton x star) singleton) f\
        ( implies (propEq singleton x star)
            ( letIn (propEq  singleton star x) (eqId singleton x) _\
             (propEq singleton 
                    (fun_app f (eqId singleton star)) 
                    (fun_app f (eqId singleton star)) 
            )
            )
        )
        ) col ext).


testD 
 :- spy( 
