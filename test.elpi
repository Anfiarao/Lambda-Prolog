test l A Q Qy
 :- Long = (
        forall 
            (setPi 
                (setPi singleton x\ propEq singleton star x) 
                _\singleton
            ) f\
            (implies (propEq singleton x star) 
            (implies (propEq singleton star x)
                (propEq singleton 
                    (app f (lambda singleton x\ (eqId singleton star))) 
                    star) 
            ))
        ),
    %spy(isType Long A ext),
    print "|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||\n",
    spy(trad Long Q),
    print "|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||\n"
 % ,  spy(isType Q Qy int)
    .

test i A B C 
 :- Long = ( forall singleton x\ forall singleton y\ 
    (implies 
        (propEq singleton x y) (implies 
        (propEq singleton y x) (implies 
        (propEq singleton x star) (implies 
        (propEq singleton y star) (implies 
        (propEq singleton star x) (implies 
        (propEq singleton star y) (
            forall 
                (setPi (propEq singleton x star) d\ 
                    setPi (propEq singleton x x) h\
                        (propEq
                            (propEq singleton x star)
                                d
                                h
                            )) f\
                propEq
                    (propEq
                        (propEq singleton star star)
                            (eqId singleton y)
                            (eqId singleton x))
                    (app 
                        (app f (eqId singleton y))
                        (eqId singleton x))
                    (app 
                        (app f (eqId singleton x))
                        (eqId singleton x))
            )
        )))))))     
    ,
    spy(isType Long A ext),
    print "|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||\n",
    spy(trad Long B),
    print "|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||\n",
    spy(isType B C int)
    .




